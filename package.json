{
  "name": "png-dex",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "dev:emulators": "cd firebase && firebase emulators:start",
    "dev:functions": "cd firebase && firebase emulators:start --only functions",
    "kill-ports:emulator": "concurrently \"npx kill-port 5001\" \"npx kill-port 9099\" \"npx kill-port 8080\"",
    "deploy:functions": "cd firebase/functions/src && npm run deploy",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject"
  },
  "dependencies": {
    "expo": "~44.0.0",
    "expo-status-bar": "~1.2.0",
    "firebase": "^9.6.7",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "!react-native-web": "0.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "concurrently": "^7.0.0",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "private": true
}